-wrap: wrap;\n\n  width: 944px;\n}\n","styles/blocks/card.scss":"@use \"../units/variables\" as *;\n\n.card {\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start ;\n  align-items: center;\n  margin-right: 48px;\n  margin-bottom: 50px;\n  width: 200px;\n  padding: 32px 16px 16px;\n  border: 1px solid #f3f3f3;\n  border-radius: 5px;\n\n  &:nth-child(4),\n  &:nth-child(8) {\n    margin-right: 0;\n  }\n\n  &__photo {\n    height: 134px;\n    width: 160px;\n    margin-bottom: 40px;\n  }\n\n  &__name {\n    margin: 0;\n    margin-bottom: 3.5px;\n    font-family: inherit;\n    font-weight: 500;\n    font-size: 12px;\n    line-height: 18px;\n    color: $main-accent;\n  }\n\n  &__code {\n    margin-block-start: 0;\n    margin-bottom: 15.5px;\n    line-height: 14px;\n    font-size: 10px;\n    font-weight: 400;\n    font-family: inherit;\n    color: #616070;\n  }\n\n  &__rating {\n    display: flex;\n    margin-bottom: 22.5px;\n    width: 100%;\n    justify-content: space-between;\n    align-items: center;\n  }\n\n  &__feedback {\n    margin: 0;\n    font-size: 10px;\n    font-weight: 400;\n    line-height: 14px;\n    color: $main-accent;\n  }\n\n  &__price-value {\n    height: 18px;\n    margin-bottom: 18px;\n    display: flex;\n    width: 100%;\n    justify-content: space-between;\n    align-items: baseline;\n  }\n\n  &__price {\n    margin-block-start: 0;\n    font-size: 12px;\n    line-height: 18px;\n    font-weight: 400;\n    color: #616070;\n  }\n  \n  &__value {\n    margin-block-start: 0;\n    font-size: 16px;\n    font-weight: bold;\n    color: $main-accent;\n  }\n  \n  &__buy-button {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    width: 100%;\n    height: 40px;\n    font-size: 14px;\n    font-weight: bold;\n    line-height: 16px;\n    border-radius: 5px;\n    text-transform: uppercase;\n    text-decoration: none;\n    color: #fff;\n    background: #00acdc;\n  }\n  \n  &__buy-button:hover {\n    background-color: #fff;\n    border: 1px solid $blue-accent;\n    color: $blue-accent;\n  }\n}\n\n.stars__star {\n  width: 16px;\n  height: 16px;\n  margin-right: 4px;\n\n  background: url(images/star.svg) no-repeat center;\n}\n\n@for $index from 1 through 5 {\n  .stars--#{$index} {\n    display: flex;\n  }\n}\n\n.stars--1 .stars__star:nth-child(1),\n.stars--2 .stars__star:nth-child(-n+2),\n.stars--3 .stars__star:nth-child(-n+3),\n.stars--4 .stars__star:nth-child(-n+4),\n.stars--5 .stars__star:nth-child(n) {\n  background: url(images/star-active.svg) no-repeat center;\n}\n","styles/units/_variables.scss":"$main-accent: #060b35;\n$blue-accent: #00acdc;\n"},"lineCount":null}},"error":null,"hash":"ba85f4bba198541efab4d1512976d214","cacheData":{"env":{}}}