scss":".logo {\n  height: 40px;\n}\n","styles\\components\\_nav.scss":".nav {\n  // test\n  &__list {\n    display: flex;\n  }\n\n  &__list-item {\n    margin-left: 20px;\n  }\n\n  &__list-link {\n    display: inline-block;\n    text-transform: uppercase;\n    font-weight: $primary-fw;\n    font-size: $secondary-fs;\n    color: #000;\n    line-height: 60px;\n    position: relative;\n  }\n\n  &__list-link:hover {\n    color: $secondary-color;\n  }\n\n  .is-active {\n    color: $secondary-color;\n  }\n\n  .is-active::after {\n    content: \"\";\n    display: block;\n    height: 4px;\n    width: $max-width;\n    background-color: $secondary-color;\n    position: absolute;\n    bottom: 0;\n    border-radius: 8px;\n  }\n\n  &__list-item:first-child {\n    margin-left: 0;\n  }\n}\n","styles\\utils\\_variables.scss":"$font-stack: \"Roboto\", sans-serif;\n$breakpoint-md: 1024px;\n$primary-color: #fff;\n$secondary-color: #00acdc;\n$primary-fs: 11px;\n$secondary-fs: 12px;\n$primary-fw: 500;\n$secondary-fw: 700;\n$primary-lh: 13px;\n$max-width: 100%;\n$text-uppercase: uppercase;\n","styles\\components\\_card.scss":".card {\n  display: grid;\n  grid-template-columns: repeat(4, 200px);\n  justify-content: space-between;\n  grid-row-gap: 50px;\n\n  &__block {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    padding: 15px 14px;\n    border: 1px solid #f3f3f3;\n    border-radius: 5px;\n  }\n\n  &__block-img {\n    // max-width: 160px;\n    height: 164px;\n    padding: 12px 2px;\n    display: flex;\n    margin-bottom: 30px;\n    border: 3px solid transparent;\n    transition: transform 1s;\n  }\n\n  &__block-img:hover {\n    border: 3px solid #f3f3f3;\n    border-top-left-radius: 5px;\n    border-top-right-radius: 5px;\n    transform: scale(1.2);\n  }\n\n  &__img {\n    width: $max-width;\n    height: auto;\n  }\n\n  &__block-info {\n    margin-bottom: 14px;\n  }\n\n  &__info-heading {\n    font-weight: $primary-fw;\n    font-size: $secondary-fs;\n    line-height: 15px;\n    margin-bottom: 6px;\n  }\n\n  &__info-code {\n    line-height: $primary-lh;\n  }\n\n  &__block-rating {\n    width: $max-width;\n    @include dflex-between-center;\n    margin-bottom: 20px;\n  }\n\n  &__rating-stars {\n    display: flex;\n  }\n\n  &__rating-feedback {\n    line-height: $primary-lh;\n  }\n\n  &__block-price {\n    width: $max-width;\n    @include dflex-between-center;\n    margin-bottom: 20px;\n  }\n\n  &__price-name {\n    line-height: $primary-lh;\n    margin-top: 3px;\n  }\n\n  &__price-number {\n    font-weight: $secondary-fw;\n    font-size: 16px;\n    line-height: 19px;\n  }\n\n  &__block-btn {\n    width: $max-width;\n  }\n\n  &__btn {\n    font-family: inherit;\n    width: $max-width;\n    height: 40px;\n    font-weight: $secondary-fw;\n    font-size: 14px;\n    line-height: 16px;\n    text-transform: uppercase;\n    color: $primary-color;\n    background-color: $secondary-color;\n    border: 1px solid $secondary-color;\n    border-radius: 5px;\n  }\n\n  &__btn:hover {\n    color: $secondary-color;\n    background-color: $primary-color;\n    border: 1px solid $secondary-color;\n  }\n}\n","styles\\utils\\_mixin.scss":"@mixin dflex-between-center {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n}\n","styles\\components\\_card-stars.scss":".card {\n  &__star {\n    width: 15px;\n    height: 15px;\n    margin-right: 4px;\n    background: url(../images/star.svg) no-repeat center;\n  }\n\n  @for $i from 1 through 5 {\n    .stars--#{$i} {\n      .card__star:nth-child(-n + #{$i}) {\n        background: url(../images/star-active.svg) no-repeat center;\n      }\n    }\n  }\n}\n","styles\\components\\_responsive.scss":"@media only screen and (max-width: $breakpoint-md) {\n  .container-main {\n    width: calc(100% - 80px);\n  }\n}\n","styles\\main.scss":"@import \"./utils/reset\";\n@import \"./utils/variables\";\n@import \"./utils/mixin.scss\";\n@import \"./components/container\";\n@import \"./components/header\";\n@import \"./components/logo\";\n@import \"./components/nav\";\n@import \"./components/card\";\n@import \"./components/card-stars\";\n@import \"./components/responsive\";\n\nhtml {\n  font-family: $font-stack;\n  font-size: $primary-fs;\n}\n"},"lineCount":null}},"error":null,"hash":"50e01a32061416b6cd8fd4f2110192d7","cacheData":{"env":{}}}